{
  "name": "databridge",
  "version": "0.1.0",
  "description": "Data bridging software to modularize, automate and schedule the transfer of data between different sources and destinations.",
  "main": "app.js",
  "scripts": {
    "test": "mocha spec --recursive",
    "start": "node app",
    "clean": "node bin/clean",
    "deploy": "git add . && git commit -m \"npm deploy\" && git push origin master",
    "pullinstall": "git pull && npm install",
    "test_temp": "node app -s oracle -t employees.ferpa_certified -d mssql"
  },
  "author": "Michael A Smith msmith104@uaa.alaska.edu @psalmody psalmody@gmail.com",
  "license": "MIT",
  "repository": "git@gitlab.com:databridge/databridge.git",
  "dependencies": {
    "async": "^1.5.2",
    "cli-spinner": "^0.2.5",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "databridge-destination-csv": "git+ssh://git@gitlab.com/databridge/databridge-destination-csv.git",
    "databridge-destination-json": "git+ssh://git@gitlab.com/databridge/databridge-destination-json.git",
    "databridge-destination-mongo": "git+ssh://git@gitlab.com/databridge/databridge-destination-mongo.git",
    "databridge-destination-mssql": "git+ssh://git@gitlab.com/databridge/databridge-destination-mssql.git",
    "databridge-destination-mysql": "git+ssh://git@gitlab.com/databridge/databridge-destination-mysql.git",
    "databridge-source-csv": "git+ssh://git@gitlab.com/databridge/databridge-source-csv.git",
    "databridge-source-mssql": "git+ssh://git@gitlab.com/databridge/databridge-source-mssql.git",
    "databridge-source-mysql": "git+ssh://git@gitlab.com/databridge/databridge-source-mysql.git",
    "databridge-source-oracle": "git+ssh://git@gitlab.com/databridge/databridge-source-oracle.git",
    "databridge-source-xlsx": "git+ssh://git@gitlab.com/databridge/databridge-source-xlsx.git",
